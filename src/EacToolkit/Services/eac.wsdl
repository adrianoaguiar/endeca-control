<?xml version="1.0" encoding="UTF-8"?>

<definitions name="endeca.wsdl"
             targetNamespace="http://www.endeca.com/EAC/Nov2006"
             xmlns="http://schemas.xmlsoap.org/wsdl/"
             xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
             xmlns:tns="http://www.endeca.com/EAC/Nov2006"
             xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">
  <types>
    <schema elementFormDefault="qualified"
            targetNamespace="http://www.endeca.com/EAC/Nov2006"
            xmlns="http://www.w3.org/2001/XMLSchema">

      <!-- General Types -->
      <simpleType name="IDType">
        <restriction base="string" />
      </simpleType>

      <complexType name="ScriptType">
        <sequence>
          <element name="scriptID" type="tns:IDType" minOccurs="1" maxOccurs="1" />
          <element name="cmd" type="string" minOccurs="1" maxOccurs="1" />
          <element name="workingDir" type="string" minOccurs="0" maxOccurs="1" />
          <element name="logFile" type="string" minOccurs="0" maxOccurs="1" />
        </sequence>
      </complexType>

      <complexType name="ScriptListType">
        <sequence>
          <element minOccurs="1" maxOccurs="unbounded"
                   name="script" type="tns:ScriptType" />
        </sequence>
      </complexType>

      <complexType name="PropertyType">
        <sequence>
          <element name="name" type="string" minOccurs="1" maxOccurs="1" />
          <element name="value" type="string" minOccurs="0" maxOccurs="1" />
        </sequence>
      </complexType>

      <complexType name="PropertyListType">
        <sequence>
          <element minOccurs="1" maxOccurs="unbounded"
                   name="property" type="tns:PropertyType" />
        </sequence>
      </complexType>

      <complexType name="DirectoryType">
        <sequence>
          <element name="dirID" type="string" minOccurs="1" maxOccurs="1" />
          <element name="dir" type="string" minOccurs="1" maxOccurs="1" />
        </sequence>
      </complexType>

      <complexType name="DirectoryListType">
        <sequence>
          <element minOccurs="1" maxOccurs="unbounded"
                   name="directory" type="tns:DirectoryType" />
        </sequence>
      </complexType>

      <complexType name="HostType">
        <sequence>
          <element minOccurs="1" maxOccurs="1" name="hostname"
                   type="string" />
          <element minOccurs="1" maxOccurs="1" name="port"
                   type="tns:PortNumber" />
          <element minOccurs="0" maxOccurs="1" name="hostID"
                   type="tns:IDType" />
          <element minOccurs="0" maxOccurs="1" name="properties"
                   type="tns:PropertyListType" />
          <element minOccurs="0" maxOccurs="1" name="directories"
                   type="tns:DirectoryListType" />
        </sequence>
      </complexType>

      <complexType name="HostListType">
        <sequence>
          <element minOccurs="1" maxOccurs="unbounded"
                   name="host" type="tns:HostType" />
        </sequence>
      </complexType>

      <complexType name="FullyQualifiedComponentIDType">
        <sequence>
          <element minOccurs="1" maxOccurs="1"
                   name="applicationID" type="tns:IDType" />
          <element minOccurs="1" maxOccurs="1"
                   name="componentID" type="tns:IDType" />
        </sequence>
      </complexType>

      <complexType name="FullyQualifiedScriptIDType">
        <sequence>
          <element name="applicationID" type="tns:IDType"
                   minOccurs="1" maxOccurs="1" />
          <element name="scriptID" type="tns:IDType"
                   minOccurs="1" maxOccurs="1" />
        </sequence>
      </complexType>

      <complexType name="FullyQualifiedHostIDType">
        <sequence>
          <element minOccurs="1" maxOccurs="1"
                   name="applicationID" type="tns:IDType" />
          <element minOccurs="1" maxOccurs="1"
                   name="hostID" type="tns:IDType" />
        </sequence>
      </complexType>


      <simpleType name="StateType">
        <restriction base="string">
          <enumeration value="Starting" />
          <enumeration value="Running" />
          <enumeration value="NotRunning" />
          <enumeration value="Failed" />
        </restriction>
      </simpleType>

      <complexType name="StatusType">
        <sequence>
          <element maxOccurs="1" minOccurs="1"
                   name="state" type="tns:StateType" />
          <element maxOccurs="1" minOccurs="1"
                   name="startTime" type="dateTime" />
          <element minOccurs="0" maxOccurs="1"
                   name="failureMessage" type="string" />
        </sequence>
      </complexType>

      <element name="componentStatus"
               type="tns:StatusType" />
      <element name="scriptStatus"
               type="tns:StatusType" />
      <element name="token"
               type="tns:TokenType" />
      <element name="status" type="string" />

      <element name="boolean" type="boolean" />

      <complexType name="ProvisioningMessageType">
        <sequence>
          <element maxOccurs="1" minOccurs="1"
                   name="message" type="string" />
        </sequence>
      </complexType>

      <complexType name="ProvisioningErrorType">
        <complexContent>
          <extension base="tns:ProvisioningMessageType">
            <sequence />
          </extension>
        </complexContent>
      </complexType>

      <complexType name="ProvisioningWarningType">
        <complexContent>
          <extension base="tns:ProvisioningMessageType">
            <sequence />
          </extension>
        </complexContent>
      </complexType>

      <complexType name="ProvisioningWarningListType">
        <sequence>
          <element minOccurs="0" maxOccurs="unbounded"
                   name="warning" type="tns:ProvisioningWarningType" />
        </sequence>
      </complexType>

      <element name="provisioningWarningList" type="tns:ProvisioningWarningListType" />

      <complexType name="EACFault" />

      <element name="eacFault" type="tns:EACFault" />

      <complexType name="ProvisioningFault">
        <complexContent>
          <extension base="tns:EACFault">
            <sequence>
              <element name="errors" type="tns:ProvisioningErrorType" minOccurs="0" maxOccurs="unbounded" />
              <element name="warnings" type="tns:ProvisioningWarningType" minOccurs="0" maxOccurs="unbounded" />
            </sequence>
          </extension>
        </complexContent>
      </complexType>
      <element name="provisioningFault" type="tns:ProvisioningFault" />


      <complexType name="ServerStatusType">
        <complexContent>
          <extension base="tns:StatusType">
          </extension>
        </complexContent>
      </complexType>

      <complexType name="BatchStatusType">
        <complexContent>
          <extension base="tns:StatusType">
            <sequence>
              <element maxOccurs="1" minOccurs="1"
                       name="duration" type="duration" />
            </sequence>
          </extension>
        </complexContent>
      </complexType>

      <!-- Provisioning & App Definition Types -->

      <complexType name="ApplicationIDListType">
        <sequence>
          <element minOccurs="0" maxOccurs="unbounded"
                   name="applicationID" type="tns:IDType" />
        </sequence>
      </complexType>

      <complexType name="ApplicationType">
        <sequence>
          <element name="applicationID"
                   type="tns:IDType" />
          <element name="hosts" type="tns:HostListType"
                   minOccurs="0" maxOccurs="1" />
          <element name="components" type="tns:ComponentListType"
                   minOccurs="0" maxOccurs="1" />
          <element name="scripts" type="tns:ScriptListType"
                   minOccurs="0" maxOccurs="1" />
        </sequence>
      </complexType>

      <complexType name="AddHostType">
        <sequence>
          <element minOccurs="1" maxOccurs="1" name="applicationID"
                   type="tns:IDType" />
          <element minOccurs="1" maxOccurs="1" name="host"
                   type="tns:HostType" />
        </sequence>
      </complexType>

      <complexType name="AddComponentType">
        <sequence>
          <element minOccurs="1" maxOccurs="1"
                   name="applicationID" type="tns:IDType" />
          <element minOccurs="1" maxOccurs="1"
                   name="component" type="tns:ComponentType" />
        </sequence>
      </complexType>

      <complexType name="AddScriptType">
        <sequence>
          <element minOccurs="1" maxOccurs="1"
                   name="applicationID" type="tns:IDType" />
          <element minOccurs="1" maxOccurs="1"
                   name="script" type="tns:ScriptType" />
        </sequence>
      </complexType>

      <complexType name="UpdateHostType">
        <sequence>
          <element minOccurs="1" maxOccurs="1" name="applicationID"
                   type="tns:IDType" />
          <element minOccurs="1" maxOccurs="1" name="host"
                   type="tns:HostType" />
          <element minOccurs="0" maxOccurs="1"
                   name="forceUpdate" type="boolean" />
        </sequence>
      </complexType>

      <complexType name="UpdateComponentType">
        <sequence>
          <element minOccurs="1" maxOccurs="1"
                   name="applicationID" type="tns:IDType" />
          <element minOccurs="1" maxOccurs="1"
                   name="component" type="tns:ComponentType" />
          <element minOccurs="0" maxOccurs="1"
                   name="forceUpdate" type="boolean" />
        </sequence>
      </complexType>

      <complexType name="UpdateScriptType">
        <sequence>
          <element minOccurs="1" maxOccurs="1"
                   name="applicationID" type="tns:IDType" />
          <element minOccurs="1" maxOccurs="1"
                   name="script" type="tns:ScriptType" />
          <element minOccurs="0" maxOccurs="1"
                   name="forceUpdate" type="boolean" />
        </sequence>
      </complexType>

      <complexType name="RemoveApplicationType">
        <sequence>
          <element minOccurs="1" maxOccurs="1"
                   name="applicationID" type="tns:IDType" />
          <element minOccurs="0" maxOccurs="1"
                   name="forceRemove" type="boolean" />
        </sequence>
      </complexType>

      <complexType name="RemoveHostType">
        <sequence>
          <element minOccurs="1" maxOccurs="1"
                   name="host" type="tns:FullyQualifiedHostIDType" />
          <element minOccurs="0" maxOccurs="1"
                   name="forceRemove" type="boolean" />
        </sequence>
      </complexType>

      <complexType name="RemoveComponentType">
        <sequence>
          <element minOccurs="1" maxOccurs="1"
                   name="component" type="tns:FullyQualifiedComponentIDType" />
          <element minOccurs="0" maxOccurs="1"
                   name="forceRemove" type="boolean" />
        </sequence>
      </complexType>

      <complexType name="RemoveScriptType">
        <sequence>
          <element minOccurs="1" maxOccurs="1"
                   name="applicationID" type="tns:IDType" />
          <element minOccurs="1" maxOccurs="1"
                   name="scriptID" type="tns:IDType" />
          <element minOccurs="0" maxOccurs="1"
                   name="forceRemove" type="boolean" />
        </sequence>
      </complexType>

      <simpleType name="PortNumber">
        <restriction base="int">
          <minInclusive value="1" />
          <maxExclusive value="65536" />
        </restriction>
      </simpleType>

      <simpleType name="TimeOut">
        <restriction base="long">
          <minInclusive value="1" />
        </restriction>
      </simpleType>

      <complexType name="ComponentType">
        <sequence>
          <element name="componentID" type="tns:IDType" minOccurs="1" maxOccurs="1" />
          <element name="hostID" type="tns:IDType" minOccurs="1" maxOccurs="1" />
          <element name="workingDir" type="string" minOccurs="0" maxOccurs="1" />
          <element name="logFile" type="string" minOccurs="0" maxOccurs="1" />
          <element name="properties" type="tns:PropertyListType" minOccurs="0" maxOccurs="1" />
        </sequence>
      </complexType>

      <complexType name="ArgListType">
        <sequence>
          <element minOccurs="0" maxOccurs="unbounded"
                   name="arg" type="string" />
        </sequence>
      </complexType>

      <complexType name="JavaOptionListType">
        <sequence>
          <element minOccurs="0" maxOccurs="unbounded"
                   name="javaOption" type="string" />
        </sequence>
      </complexType>

      <complexType name="ClasspathType">
        <sequence>
          <element minOccurs="0" maxOccurs="unbounded"
                   name="classpathElement" type="string" />
        </sequence>
      </complexType>

      <complexType name="SSLConfigurationType">
        <sequence>
          <element name="certFile" type="string" minOccurs="1" maxOccurs="1" />
          <element name="caFile" type="string" minOccurs="1" maxOccurs="1" />
          <element name="cipher" type="string" minOccurs="0" maxOccurs="1" />
        </sequence>
      </complexType>

      <complexType name="DgraphComponentType">
        <complexContent>
          <extension base="tns:ComponentType">
            <sequence>
              <element name="args" type="tns:ArgListType"
                       minOccurs="0" maxOccurs="1" />
              <element name="port" type="tns:PortNumber"
                       minOccurs="1" maxOccurs="1" />
              <element name="inputPrefix" type="string"
                       minOccurs="1" maxOccurs="1" />
              <element name="appConfigPrefix" type="string"
                       minOccurs="0" maxOccurs="1" />
              <element name="reqLogFile" type="string"
                       minOccurs="0" maxOccurs="1" />
              <element name="spellDir" type="string"
                       minOccurs="0" maxOccurs="1" />
              <element name="startupTimeout"
                       type="tns:TimeOut" minOccurs="0" maxOccurs="1" />
              <element name="sslConfiguration" type="tns:SSLConfigurationType"
                       minOccurs="0" maxOccurs="1" />
              <element name="updateDir" type="string"
                       minOccurs="0" maxOccurs="1" />
              <element name="updateLogFile" type="string"
                       minOccurs="0" maxOccurs="1" />
              <element name="tempDir" type="string"
                       minOccurs="0" maxOccurs="1" />
            </sequence>
          </extension>
        </complexContent>
      </complexType>

      <complexType name="AgraphChildType" />

      <complexType name="DgraphHostPortType">
        <complexContent>
          <extension base="tns:AgraphChildType">
            <sequence>
              <element name="hostname" type="string"
                       minOccurs="1" maxOccurs="1" />
              <element name="port" type="tns:PortNumber"
                       minOccurs="1" maxOccurs="1" />
            </sequence>
          </extension>
        </complexContent>
      </complexType>

      <complexType name="DgraphReferenceType">
        <complexContent>
          <extension base="tns:AgraphChildType">
            <sequence>
              <element name="componentID" type="tns:IDType"
                       minOccurs="1" maxOccurs="1" />
            </sequence>
          </extension>
        </complexContent>
      </complexType>

      <complexType name="AgraphChildListType">
        <sequence>
          <element minOccurs="1" maxOccurs="unbounded"
                   name="child" type="tns:AgraphChildType" />
        </sequence>
      </complexType>

      <complexType name="AgraphComponentType">
        <complexContent>
          <extension base="tns:ComponentType">
            <sequence>
              <element name="args" type="tns:ArgListType"
                       minOccurs="0" maxOccurs="1" />
              <element name="port" type="tns:PortNumber"
                       minOccurs="1" maxOccurs="1" />
              <element name="appConfigPrefix" type="string"
                       minOccurs="0" maxOccurs="1" />
              <element name="reqLogFile" type="string"
                       minOccurs="0" maxOccurs="1" />
              <element name="children"
                       type="tns:AgraphChildListType" minOccurs="1" maxOccurs="1" />
              <element name="inputPrefix" type="string"
                       minOccurs="1" maxOccurs="1" />
              <element name="startupTimeout"
                       type="tns:TimeOut" minOccurs="0" maxOccurs="1" />
              <element name="sslConfiguration" type="tns:SSLConfigurationType"
                       minOccurs="0" maxOccurs="1" />
            </sequence>
          </extension>
        </complexContent>
      </complexType>

      <complexType name="LogServerComponentType">
        <complexContent>
          <extension base="tns:ComponentType">
            <sequence>
              <element name="port" type="tns:PortNumber"
                       minOccurs="1" maxOccurs="1" />
              <element name="outputPrefix" type="string"
                       minOccurs="1" maxOccurs="1" />
              <element name="gzip" type="boolean"
                       minOccurs="1" maxOccurs="1" />
              <element name="startupTimeout"
                       type="tns:TimeOut" minOccurs="0" maxOccurs="1" />
            </sequence>
          </extension>
        </complexContent>
      </complexType>

      <complexType name="ForgeComponentType">
        <complexContent>
          <extension base="tns:ComponentType">
            <sequence>
              <element name="args" type="tns:ArgListType"
                       minOccurs="0" maxOccurs="1" />
              <element name="stateDir" type="string"
                       minOccurs="0" maxOccurs="1" />
              <element name="inputDir" type="string"
                       minOccurs="0" maxOccurs="1" />
              <element name="outputDir" type="string"
                       minOccurs="0" maxOccurs="1" />
              <element name="numPartitions" type="int"
                       minOccurs="0" maxOccurs="1" />
              <element name="pipelineFile" type="string"
                       minOccurs="1" maxOccurs="1" />
              <element name="outputPrefixName" type="string"
                       minOccurs="0" maxOccurs="1" />
              <element name="tempDir" type="string"
                       minOccurs="0" maxOccurs="1" />
              <element name="webServicePort" type="tns:PortNumber"
                       minOccurs="0" maxOccurs="1" />
              <element name="sslConfiguration" type="tns:SSLConfigurationType"
                       minOccurs="0" maxOccurs="1" />
            </sequence>
          </extension>
        </complexContent>
      </complexType>

      <complexType name="DgidxComponentType">
        <complexContent>
          <extension base="tns:ComponentType">
            <sequence>
              <element name="args" type="tns:ArgListType"
                       minOccurs="0" maxOccurs="1" />
              <element name="appConfigPrefix" type="string"
                       minOccurs="0" maxOccurs="1" />
              <element name="inputPrefix" type="string"
                       minOccurs="1" maxOccurs="1" />
              <element name="outputPrefix" type="string"
                       minOccurs="1" maxOccurs="1" />
              <element name="runAspell" type="boolean"
                       minOccurs="0" maxOccurs="1" />
              <element name="tempDir" type="string"
                       minOccurs="0" maxOccurs="1" />
            </sequence>
          </extension>
        </complexContent>
      </complexType>

      <complexType name="InputPrefixListType">
        <sequence>
          <element minOccurs="1" maxOccurs="unbounded"
                   name="inputPrefix" type="string" />
        </sequence>
      </complexType>

      <complexType name="AgidxComponentType">
        <complexContent>
          <extension base="tns:ComponentType">
            <sequence>
              <element name="args" type="tns:ArgListType"
                       minOccurs="0" maxOccurs="1" />
              <element name="previousAgidxOutputPrefix"
                       type="string" minOccurs="0" maxOccurs="1" />
              <element name="inputPrefixList"
                       type="tns:InputPrefixListType" minOccurs="1" maxOccurs="1" />
              <element name="outputPrefix" type="string"
                       minOccurs="1" maxOccurs="1" />
            </sequence>
          </extension>
        </complexContent>
      </complexType>

      <complexType name="CrawlerComponentType">
        <complexContent>
          <extension base="tns:ComponentType">
            <sequence>
              <element name="args" type="tns:ArgListType"
                       minOccurs="0" maxOccurs="1" />
              <element name="javaOptions" type="tns:JavaOptionListType"
                       minOccurs="0" maxOccurs="1" />
              <element name="classpath" type="tns:ClasspathType"
                       minOccurs="0" maxOccurs="1" />
              <element name="defaultSettingsFile" type="string"
                       minOccurs="1" maxOccurs="1" />
              <element name="globalConfigFile" type="string"
                       minOccurs="1" maxOccurs="1" />
              <element name="profileConfigFile" type="string"
                       minOccurs="1" maxOccurs="1" />
              <element name="urlListFile" type="string"
                       minOccurs="1" maxOccurs="1" />
              <element name="outputPrefix" type="string"
                       minOccurs="1" maxOccurs="1" />
              <element name="port" type="tns:PortNumber"
                       minOccurs="0" maxOccurs="1" />
            </sequence>
          </extension>
        </complexContent>
      </complexType>

      <simpleType name="TimeRangeType">
        <restriction base="string">
          <enumeration value="Yesterday" />
          <enumeration value="LastWeek" />
          <enumeration value="LastMonth" />
          <enumeration value="DaySoFar" />
          <enumeration value="WeekSoFar" />
          <enumeration value="MonthSoFar" />
        </restriction>
      </simpleType>

      <simpleType name="TimeSeriesType">
        <restriction base="string">
          <enumeration value="Hourly" />
          <enumeration value="Daily" />
        </restriction>
      </simpleType>

      <complexType name="ReportGeneratorComponentType">
        <complexContent>
          <extension base="tns:ComponentType">
            <sequence>
              <element name="args" type="tns:ArgListType"
                       minOccurs="0" maxOccurs="1" />
              <element name="javaBinary" type="string"
                       minOccurs="0" maxOccurs="1" />
              <element name="javaOptions" type="tns:JavaOptionListType"
                       minOccurs="0" maxOccurs="1" />
              <element name="inputDirOrFile" type="string"
                       minOccurs="1" maxOccurs="1" />
              <element name="outputFile" type="string"
                       minOccurs="1" maxOccurs="1" />
              <element name="stylesheetFile" type="string"
                       minOccurs="1" maxOccurs="1" />
              <element name="settingsFile" type="string"
                       minOccurs="0" maxOccurs="1" />
              <element name="timerange" type="tns:TimeRangeType"
                       minOccurs="0" maxOccurs="1" />
              <element name="startDate" type="string"
                       minOccurs="0" maxOccurs="1" />
              <element name="stopDate" type="string"
                       minOccurs="0" maxOccurs="1" />
              <element name="timeSeries" type="tns:TimeSeriesType"
                       minOccurs="0" maxOccurs="1" />
              <element name="charts" type="boolean"
                       minOccurs="0" maxOccurs="1" />
            </sequence>
          </extension>
        </complexContent>
      </complexType>

      <complexType name="ComponentListType">
        <sequence>
          <element minOccurs="1" maxOccurs="unbounded"
                   name="component" type="tns:ComponentType" />
        </sequence>
      </complexType>

      <element name="application"
               type="tns:ApplicationType" />
      <element name="applicationIDList"
               type="tns:ApplicationIDListType" />

      <!-- Utility Types -->
      <simpleType name="TokenType">
        <restriction base="string" />
      </simpleType>

      <simpleType name="BackupMethodType">
        <restriction base="string">
          <enumeration value="Move" />
          <enumeration value="Copy" />
        </restriction>
      </simpleType>

      <complexType name="FullyQualifiedUtilityTokenType">
        <sequence>
          <element minOccurs="1" maxOccurs="1"
                   name="applicationID" type="tns:IDType" />
          <element minOccurs="1" maxOccurs="1"
                   name="token" type="tns:TokenType" />
        </sequence>
      </complexType>

      <complexType name="RunUtilityType">
        <sequence>
          <element minOccurs="1" maxOccurs="1" name="applicationID" type="tns:IDType" />
          <element minOccurs="0" maxOccurs="1" name="token" type="tns:TokenType" />
        </sequence>
      </complexType>

      <complexType name="RunShellType">
        <complexContent>
          <extension base="tns:RunUtilityType">
            <sequence>
              <element minOccurs="1" maxOccurs="1"
                       name="hostID" type="tns:IDType" />
              <element name="cmd" type="string"
                       minOccurs="1" maxOccurs="1" />
              <element name="workingDir" type="string"
                       minOccurs="0" maxOccurs="1" />
            </sequence>
          </extension>
        </complexContent>
      </complexType>

      <complexType name="RunFileCopyType">
        <complexContent>
          <extension base="tns:RunUtilityType">
            <sequence>
              <element minOccurs="1" maxOccurs="1"
                       name="fromHostID" type="tns:IDType" />
              <element minOccurs="1" maxOccurs="1"
                       name="toHostID" type="tns:IDType" />
              <element minOccurs="1" maxOccurs="1"
                       name="sourcePath" type="string" />
              <element minOccurs="1" maxOccurs="1"
                       name="destinationPath" type="string" />
              <element minOccurs="0" maxOccurs="1"
                       name="recursive" type="boolean" />
            </sequence>
          </extension>
        </complexContent>
      </complexType>

      <complexType name="RunBackupType">
        <complexContent>
          <extension base="tns:RunUtilityType">
            <sequence>
              <element minOccurs="1" maxOccurs="1"
                       name="hostID" type="tns:IDType" />
              <element minOccurs="1" maxOccurs="1"
                       name="dirName" type="string" />
              <element minOccurs="0" maxOccurs="1"
